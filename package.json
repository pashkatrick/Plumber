{
    "name": "Plumber",
    "version": "2.0.0",
    "description": "A really missing GUI for GRPC requests",
    "author": "pashkatrick",
    "main": "main.js",
    "homepage": "https://pshktrck.ru/plumber/",
    "license": "MIT",
    "scripts": {
        "start": "electron .",
        "npm-install": "node_modules/.bin/electron-rebuild; npm install",
        "mac-pkg": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-build --extra-resource=./node_modules/electron/dist/Electron.app/Contents/Resources/grpcurl --icon=assets/app.icns",
        "win-pkg": "electron-packager . --overwrite --platform=win32 --arch=x64 --prune=true --out=release-build --extra-resource=./node_modules/electron/dist/resources/grpcurl --icon=assets/app.ico",
        "dmg": "electron-installer-dmg release-build/Plumber-darwin-x64/Plumber.app/ Plumber --out=release-build-dmg --overwrite --icon=assets/app.icns",
        "exe": "electron-installer-windows --src release-build/Plumber-win32-x64/ --dest release-build-exe --overwrite --icon=assets/app.ico"
    },
    "repository": "https://github.com/pashkatrick/Plumber",
    "keywords": [
        "Electron",
        "GRPC",
        "Requests",
        "Testing"
    ],
    "dependencies": {
        "app-root-dir": "^1.0.2",
        "electron-reloader": "^1.2.1",
        "fs": "0.0.1-security",
        "monaco-editor": "^0.17.0",
        "monaco-loader": "^1.0.0",
        "utf8": "^3.0.0"
    },
    "devDependencies": {
        "electron": "^4.2.12",
        "electron-installer-dmg": "^3.0.0",
        "electron-installer-windows": "^3.0.0",
        "electron-localshortcut": "^3.2.1",
        "electron-packager": "^15.2.0",
        "electron-rebuild": "^1.11.0",
        "electron-squirrel-startup": "^1.0.0"
    }
}
